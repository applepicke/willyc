.container-fluid.text-center
  %h1 Tiny Game

  .col-md-4.col-md-offset-4
    %h2 Latest Builds

    .col-md-4
      %h4 Windows x32
      - if @latest[:x32]
        = link_to 'Download', aws_url(@latest[:x32].url)

    .col-md-4
      %h4 Windows x64
      - if @latest[:x64]
        = link_to 'Download', aws_url(@latest[:x64].url)

    .col-md-4
      %h4 OSX
      - if @latest[:osx]
        = link_to 'Download', aws_url(@latest[:osx].url)

